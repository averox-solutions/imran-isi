<include>
    <extension name="avx_webrtc_call" continue="true">
      <condition field="${sip_user_agent}" expression="avx-webrtc-sfu" break="on-false">
        <action application="set" data="presence_data=from_avx-webrtc-sfu"/>
        <action application="set" data="avx_authorized=true"/>
        <action application="set" data="rtp_manual_rtp_bugs=ACCEPT_ANY_PACKETS"/>
        <action application="set" data="jb_use_timestamps=true"/>
        <action application="transfer" data="${destination_number} XML default"/>
      </condition>
    </extension>
</include>
